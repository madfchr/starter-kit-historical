{
  "name": "starter-kit",
  "version": "1.0.0",
  "description": "UENO. starter kit",
  "engines": {
    "node": ">=6.0 <7",
    "npm": ">=3.8 <4"
  },
  "main": "index.js",
  "scripts": {
    "start": "node ./build/server.js",
    "dev": "node ./webpack/dev.js",
    "test": "karma start ./webpack/karma.js",
    "lint": "npm run eslint; npm run stylelint",
    "eslint": "eslint src",
    "stylelint": "stylelint \"src/**/*.less\" --syntax less",
    "build": "rm -rf ./build && cross-env NODE_ENV=production webpack -p --config ./webpack/release.js",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ueno-llc/starter-kit.git"
  },
  "keywords": [
    "react",
    "webpack",
    "starter-kit",
    "universal"
  ],
  "author": "UENO.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ueno-llc/starter-kit/issues"
  },
  "homepage": "https://github.com/ueno-llc/starter-kit#readme",
  "dependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "bundle-loader": "^0.5.4",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "ejs": "^2.4.2",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "postcss-loader": "^0.10.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-helmet": "^3.1.0",
    "react-hot-loader": "^1.3.0",
    "react-router": "^2.4.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.4",
